{
  "expo": {
    "name": "ReadyOrNot",
    "slug": "ready-or-not",
    "scheme": "mycoolredirect",
    "privacy": "public",
    "sdkVersion": "49.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nosuchcolor.readyornot",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["com.googleusercontent.apps.1014185678010-n27tuaikhclnke2vojbn7fum81qq3vh4"]
          }
        ]
      }
    },
    "android": {
      "package": "com.nosuchcolor.readyornot"
    },
    "plugins": ["@react-native-google-signin/google-signin"]
  }
}
